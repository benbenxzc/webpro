<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>SERVLET.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>SERVLET.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PromotionalActivitiesServlet</servlet-name>
    <servlet-class>SERVLET.PromotionalActivitiesServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditActivitiesServlet</servlet-name>
    <servlet-class>SERVLET.EditActivitiesServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>p</servlet-name>
    <servlet-class>SERVLET.p</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>dalian</servlet-name>
    <servlet-class>SERVLET.dalian</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UploadImages</servlet-name>
    <servlet-class>SERVLET.UploadImages</servlet-class>
  </servlet>







  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/servlet/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/servlet/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PromotionalActivitiesServlet</servlet-name>
    <url-pattern>/servlet/PromotionalActivitiesServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditActivitiesServlet</servlet-name>
    <url-pattern>/servlet/EditActivitiesServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>p</servlet-name>
    <url-pattern>/servlet/p</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>dalian</servlet-name>
    <url-pattern>/servlet/dalian</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UploadImages</servlet-name>
    <url-pattern>/servlet/UploadImages</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
